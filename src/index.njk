        <ul class="list">
        {% for post in collections.polaroids %}
            <li>
                <figure>
                    <img width="300" height="365" loading="lazy" src="{{ post.data.Image_Src }}" onmouseover="this.src= '{{ post.data.Hover_Image_Src }}'" onmouseout="this.src='{{ post.data.Image_Src }}'" alt="">
                    <figcaption>
                        <h2>
                            {{ post.data.Musician_Name }}
                            <span>{{ post.data.Band_Name }}</span>
                        </h2>
                    </figcaption>
                </figure>
            </li>
        {% endfor %}
        </ul>

